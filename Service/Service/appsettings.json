{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Data": {
    "Quant": {
      "ConnectionString": "tcp:{your_url},1433;Initial Catalog={your_catalog};Persist Security Info=False;User ID={your_user};Password={your_password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
    }
  },
  "ApiKeys": {
    "IEX": {
      "PublishableToken": "<your_publishable_token>",
      "SecurityToken": "<your_security(secret)_token>"
    },
    "AlphaVantage": {
      "ApiKey": "<your_api_key>"
    },
    "Quandl": {
      "ApiKey": "<your_api_key>"
    }
  }
}
